#include <iostream>

class Audi {
    public:
        bool startEngine(){
            if (startElectronicServices() && startLightSystem()){
                std::cout << "Audi Engine started successfully." << std::endl;
                return true;
            } else {
                std::cout << "Audi Engine failed." << std::endl;
                return false;
            }
        }
    private:
        bool startElectronicServices(){
            std::cout << "Audi Electronic Service started." << std::endl;
            return true;
        }
        bool startLightSystem(){
            std::cout << "Audi Light System started." << std::endl;
            return true;
        }
};

int main(){
    Audi b;
    bool r = b.startEngine();
    return 0;
